    },
    {
      "cell_type": "code",
      "source": [
        "d1=pd.DataFrame(hb1['agent'].value_counts()).reset_index().rename(columns={'index':'agent','agent':'num_of_bookings'}).sort_values(by='num_of_bookings',ascending=False)\n",
        "d1.drop(d1[d1['agent']==0].index,inplace=True)\n",
        "d1=d1[:10]\n",
